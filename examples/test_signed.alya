; Test Signed Comparisons

; Test 1: -1 < 1 (Signed)
@r0 := -1
@r1 := 1
if @r0 < @r1 goto test1_pass
@r9 := 0
print @r9 ; fail
halt
test1_pass:
@r9 := 1
print @r9 ; pass

; Test 2: 1 > -1 (Signed)
if @r1 > @r0 goto test2_pass
@r9 := 0
print @r9 ; fail
halt
test2_pass:
@r9 := 1
print @r9 ; pass

; Test 3: -5 < -2 (Signed)
@r2 := -5
@r3 := -2
if @r2 < @r3 goto test3_pass
@r9 := 0
print @r9 ; fail
halt
test3_pass:
@r9 := 1
print @r9 ; pass

halt
